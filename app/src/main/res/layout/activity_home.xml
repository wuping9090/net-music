<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.HomeActivity">

    <com.wuping.netmusic.widget.HomeToolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="?actionBarSize"
        android:background="?attr/colorPrimary"
        app:leftButtonIcon="@drawable/navigation"
        app:discoverIcon="@drawable/actionbar_discover_normal"
        app:discoverSelectedIcon="@drawable/actionbar_discover_selected"
        app:musicIcon="@drawable/actionbar_music_normal"
        app:musicSelectedIcon="@drawable/actionbar_music_selected"
        app:videoIcon="@drawable/actionbar_video_normal"
        app:videoSelectedIcon="@drawable/actionbar_video_selected"
        app:rightButtonIcon="@drawable/search"/>



</RelativeLayout>